{
    "ConnectionStrings": {
      "DefaultConnection": "Data Source=AngularAspNetCoreTemplate.sqlite"
    },
    "Serilog": {
      "WriteTo": [
        { "Name": "SQLite", "Args": {"sqliteDbPath": "Data\\AngularAspNetCoreTemplate.sqlite", "storeTimestampInUtc": true}}
      ],
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Warning",
          "System": "Warning"
        }
      }
    }
  }